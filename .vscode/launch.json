{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run ComfyUI",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": "--auto-launch --listen 0.0.0.0 --multi-user"
        },
        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}/web",
            "pathMapping": {
                "/scripts": "${workspaceFolder}/web/scripts",
                "/lib": "${workspaceFolder}/web/lib",
                "/extensions/core": "${workspaceFolder}/web/extensions/core",
                "/extensions/sd-ppp": "${workspaceFolder}/custom_nodes/sd-ppp/comfy/plugins",
            }
        }
    ]
}